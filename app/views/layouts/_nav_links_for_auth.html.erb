<% if user_signed_in? %>
  <li><%= link_to 'Preferences', edit_user_registration_path%></li>
  <li><%= link_to 'Dashboard', edit_user_registration_path, class:'blue_link' %></li>
  <li><%= link_to 'Logout', destroy_user_session_path, :method=>'delete', class:'logoutnav btn btn-primary navbar_login_button fa fa-copyright' %></li>
<% else %>
  <li><%= link_to 'Sign Up', new_user_registration_path, data: { no_turbolink: true }, class:'blue_link' %></li>
  <li><%= link_to 'Login', new_user_session_path, class:'btn btn-primary navbar_login_button' %></li>


<% end %>
<% if user_signed_in? %>
  <% if current_user.try(:admin?) %>
    <li><%= link_to 'Admin', '/admin' %></li>
    <li><%= link_to 'Users', users_path %></li>
  <% end %>
<% end %>
